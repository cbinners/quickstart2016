{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "ecmaFeatures": {
    "modules": true
  },
  "rules": {
    "arrow-spacing": [2, {
        "before": true,
        "after": true
    }],
    "comma-dangle": 0,
    "react/jsx-boolean-value": 0,
    "new-cap": 0,
    "no-multi-spaces": 0,
    "no-param-reassign": [2, {"props": false}],
    "arrow-body-style": [2, "always"]
  }
}
